{
  "version": 2,
  "name": "tadao-next",
  "public": false,
  "build": {
    "env": {
      "HANKYO_ACCESS_TOKEN": "@hankyo_access_token",
      "HANKYO_SECRET_ACCESS_TOKEN": "@hankyo_secret_access_token",
      "PROJECT_SLUG": "@project_slug"
    }
  },
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ]
}
